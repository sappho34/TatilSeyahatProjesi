<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var x in Model)
            {
                <img src="@x.ImageUrl" alt=""  width="900" height="520"/>
                <p>@x.Description</p>
            }

        </div>
    </div>
</div>