@using TatilSeyahat.Models.Classes
@model List<Blog>


<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.BlogImage" alt=""  style="width:150px;height:70px"/>
        </div>
        <div class="bot-desc1">
            <p>@item.Description.Substring(0, 100) </p>
        </div>
        <div class="clear"></div>
    }
</div>

